    "@tanstack/react-query": "^5.90.21",
    "@trpc/client": "11.10.0",
    "@trpc/react-query": "11.10.0",
    "@trpc/server": "11.10.0",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
