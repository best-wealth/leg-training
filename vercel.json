{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "framework": "other",
  "installCommand": "pnpm install",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
